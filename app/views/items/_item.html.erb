
<div class="col">
    <div class="card card-center" style="width: 18rem;">
    <%img=item.image%>
    <%= image_tag img,class: "card-img-top"%>
    <div class="card-body">
        <h5 class="card-title"><%= item.name %><br> Rs <%= item.price %></h5>
        <div class="item"> <%=item.food_type%> </div>
        <p class="card-text"><%=item.description%></p>
    </div>
    </div>
</div>